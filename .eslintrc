{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "max-len": 0,
    "react/jsx-no-bind": 0,
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "arrow-parens": 0,
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "radix": 0,
    "semi": 1,
    "new-cap": 1,
    "no-unused-vars": 1,
    "no-var": 1,
    "object-shorthand": 1,
    "class-methods-use-this": 0,
    "no-nested-ternary": 0,
        "no-undef": 0,
        "no-use-before-define": 0
  },
}
